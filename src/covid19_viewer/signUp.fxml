<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="covid19_viewer.SignUpController">
   <children>
      <AnchorPane id="AnchorPane" prefHeight="656.0" prefWidth="942.0">
         <children>
            <Label layoutX="277.0" layoutY="434.0" text="Username" />
            <Label layoutX="277.0" layoutY="487.0" text="Password" />
            <TextField fx:id="user" layoutX="379.0" layoutY="427.0" prefHeight="39.0" prefWidth="224.0" promptText="username" />
            <TextField fx:id="pass" layoutX="379.0" layoutY="480.0" promptText="password" />
            <Button layoutX="603.0" layoutY="480.0" mnemonicParsing="false" onAction="#signUp" text="Next" />
            <Label layoutX="419.0" layoutY="340.0" text="Sign Up">
               <font>
                  <Font name="Berlin Sans FB" size="33.0" />
               </font>
            </Label>
            <Label layoutX="282.0" layoutY="545.0" text="Close this window if you already had an account and would like to login" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
