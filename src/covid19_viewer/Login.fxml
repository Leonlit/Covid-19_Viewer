<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="656.0" prefWidth="942.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="covid19_viewer.LoginController">
   <children>
      <Label layoutX="277.0" layoutY="434.0" text="Username" />
      <Label layoutX="277.0" layoutY="487.0" text="Password" />
      <TextField fx:id="user" layoutX="379.0" layoutY="427.0" prefHeight="39.0" prefWidth="286.0" promptText="username" />
      <TextField fx:id="pass" layoutX="379.0" layoutY="480.0" promptText="password" />
      <Button layoutX="603.0" layoutY="480.0" mnemonicParsing="false" onAction="#checkLogin" text="Next" />
      <Hyperlink layoutX="341.0" layoutY="548.0" onAction="#ShowSignUp" text="Do not have an account? Click here. " />
      <Label layoutX="453.0" layoutY="340.0" text="Login">
         <font>
            <Font name="Berlin Sans FB" size="33.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
